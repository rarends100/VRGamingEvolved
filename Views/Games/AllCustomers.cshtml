@model List<VRGamingEvolved.Models.Users>

@{
    ViewData["Title"] = "All Customers";
}

<h1>All Customers</h1>

@if (User.IsInRole("Admin") || User.IsInRole("Employee"))
{
    @foreach(var user in Model){
    <div>
    <dl class="row">
        <dt class="col-sm-2">
         @Html.DisplayNameFor(model => user.UserName)
        </dt>
        <dd class="col-sm-10">
         @Html.DisplayFor(model => user.UserName)
         </dd>
    </dl>
</div>
    }
}
else
{
    <h3>No customers present!</h3>
}